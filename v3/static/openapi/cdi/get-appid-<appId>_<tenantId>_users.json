{
  "path": "/appid-<appId>/<tenantId>/users",
  "method": "get",
  "description": "User Pagination\n",
  "headers": {
    "Authorization": {
      "required": false,
      "schema": {
        "type": "string"
      }
    },
    "cdi-version": {
      "description": "X.Y of the X.Y.Z CDI version.",
      "schema": {
        "type": "string"
      }
    }
  },
  "parameters": {
    "includeRecipeIds": {
      "name": "includeRecipeIds",
      "in": "query",
      "required": false,
      "description": "The param should take an array of comma seperated strings",
      "schema": {
        "type": "string",
        "example": "val1,val2,val3.."
      }
    },
    "paginationToken": {
      "name": "paginationToken",
      "in": "query",
      "example": "ZmE3YTA4NDEtY...MjM5MTgwMzIyMzE=",
      "schema": {
        "type": "string"
      }
    },
    "limit": {
      "name": "limit",
      "in": "query",
      "example": 1,
      "schema": {
        "type": "number"
      }
    },
    "timeJoinedOrder": {
      "name": "timeJoinedOrder",
      "in": "query",
      "schema": {
        "type": "string",
        "enum": [
          "ASC",
          "DESC"
        ]
      }
    },
    "email": {
      "in": "query",
      "name": "email",
      "schema": {
        "type": "string",
        "example": "test@example.com"
      },
      "required": false
    },
    "phone": {
      "in": "query",
      "name": "phone",
      "schema": {
        "type": "number",
        "example": 911234567890
      },
      "required": false
    },
    "recipe": {
      "in": "query",
      "name": "recipe",
      "schema": {
        "type": "string",
        "example": "emailpassword"
      },
      "required": false
    },
    "provider": {
      "in": "query",
      "name": "provider",
      "schema": {
        "type": "string",
        "example": "google"
      },
      "required": false
    }
  },
  "body": {},
  "responses": {
    "200": {
      "description": "User Pagination",
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "properties": {
              "status": {
                "type": "string",
                "enum": [
                  "OK"
                ]
              },
              "users": {
                "type": "array",
                "items": {
                  "type": "object",
                  "additionalProperties": false,
                  "properties": {
                    "user": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "string",
                          "example": "fa7a0841-b533-4478-95533-0fde890c3483"
                        },
                        "isPrimaryUser": {
                          "type": "boolean",
                          "example": true
                        },
                        "tenantIds": {
                          "type": "array",
                          "items": {
                            "type": "string",
                            "example": "customer1"
                          }
                        },
                        "timeJoined": {
                          "type": "number",
                          "example": 1623918032231
                        },
                        "emails": {
                          "type": "array",
                          "items": {
                            "type": "string",
                            "example": "johndoe@gmail.com"
                          }
                        },
                        "phoneNumbers": {
                          "type": "array",
                          "items": {
                            "type": "string",
                            "example": "+14155552671"
                          }
                        },
                        "thirdParty": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "id": {
                                "type": "string",
                                "example": "google"
                              },
                              "userId": {
                                "type": "string",
                                "example": 106347997792363870000
                              }
                            }
                          }
                        },
                        "loginMethods": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "tenantIds": {
                                "type": "array",
                                "items": {
                                  "type": "string",
                                  "example": "customer1"
                                }
                              },
                              "recipeUserId": {
                                "type": "string",
                                "example": "fa7a0841-b533-4478-95533-0fde890c3483"
                              },
                              "verified": {
                                "type": "boolean",
                                "example": true
                              },
                              "timeJoined": {
                                "type": "number",
                                "example": 1623918032231
                              },
                              "recipeId": {
                                "type": "string",
                                "enum": [
                                  "emailpassword",
                                  "passwordless",
                                  "thirdparty"
                                ]
                              },
                              "email": {
                                "type": "string",
                                "example": "johndoe@gmail.com"
                              },
                              "phoneNumber": {
                                "type": "string",
                                "example": "+14155552671"
                              },
                              "thirdParty": {
                                "type": "object",
                                "properties": {
                                  "id": {
                                    "type": "string",
                                    "example": "google"
                                  },
                                  "userId": {
                                    "type": "string",
                                    "example": 106347997792363870000
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              },
              "nextPaginationToken": {
                "type": "string",
                "example": "ZmE3YTA4ND...OzE2MjM5MTgwMzIyMzE="
              }
            }
          }
        }
      }
    },
    "400": {
      "description": "error code 400",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string"
          }
        }
      }
    },
    "401": {
      "description": "error code 401",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Invalid API key"
            ]
          }
        }
      }
    },
    "404": {
      "description": "error code 404",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Not Found"
            ]
          }
        }
      }
    },
    "500": {
      "description": "error code 500",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Internal Error"
            ]
          }
        }
      }
    }
  }
}
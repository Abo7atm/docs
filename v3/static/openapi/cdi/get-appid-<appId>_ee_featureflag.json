{
  "path": "/appid-<appId>/ee/featureflag",
  "method": "get",
  "description": "Get a list of the enabled enterprise features\n",
  "headers": {
    "Authorization": {
      "required": false,
      "schema": {
        "type": "string"
      }
    },
    "cdi-version": {
      "description": "X.Y of the X.Y.Z CDI version.",
      "schema": {
        "type": "string"
      }
    }
  },
  "body": {},
  "responses": {
    "200": {
      "description": "The operation was successful",
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "properties": {
              "status": {
                "type": "string",
                "enum": [
                  "OK"
                ]
              },
              "features": {
                "type": "array",
                "example": [
                  "feature_1",
                  "feature_2"
                ],
                "items": {
                  "type": "string"
                }
              },
              "usageStats": {
                "type": "object"
              }
            }
          }
        }
      }
    },
    "400": {
      "description": "error code 400",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string"
          }
        }
      }
    },
    "401": {
      "description": "error code 401",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Invalid API key"
            ]
          }
        }
      }
    },
    "404": {
      "description": "error code 404",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Not Found"
            ]
          }
        }
      }
    },
    "500": {
      "description": "error code 500",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Internal Error"
            ]
          }
        }
      }
    }
  }
}
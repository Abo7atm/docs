{
  "path": "/appid-<appId>/recipe/accountlinking/user/link/check",
  "method": "get",
  "description": "Check if accounts can be linked for given primary and recipe user id\n",
  "headers": {
    "rid": {
      "required": false,
      "schema": {
        "type": "string"
      }
    },
    "Authorization": {
      "required": false,
      "schema": {
        "type": "string"
      }
    },
    "cdi-version": {
      "description": "X.Y of the X.Y.Z CDI version.",
      "schema": {
        "type": "string"
      }
    }
  },
  "parameters": {
    "primaryUserId": {
      "in": "query",
      "name": "primaryUserId",
      "schema": {
        "type": "string",
        "example": "fa7a0841-b533-4478-9253-0fde890c576"
      },
      "required": true
    },
    "recipeUserId": {
      "in": "query",
      "name": "recipeUserId",
      "schema": {
        "type": "string",
        "example": "fa7a0841-b533-4478-9253-0fde890c576"
      },
      "required": true
    }
  },
  "body": {},
  "responses": {
    "200": {
      "description": "Indicates success with the status prop",
      "content": {
        "application/json": {
          "schema": {
            "oneOf": [
              {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "string",
                    "enum": [
                      "OK"
                    ]
                  },
                  "accountsAlreadyLinked": {
                    "type": "boolean",
                    "example": true
                  }
                }
              },
              {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "string",
                    "enum": [
                      "ACCOUNT_INFO_ALREADY_ASSOCIATED_WITH_ANOTHER_PRIMARY_USER_ID_ERROR"
                    ]
                  },
                  "primaryUserId": {
                    "type": "string",
                    "example": "fa7a0841-b533-4478-9253-0fde890c576"
                  },
                  "description": {
                    "type": "string"
                  }
                }
              },
              {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "string",
                    "enum": [
                      "RECIPE_USER_ID_ALREADY_LINKED_WITH_ANOTHER_PRIMARY_USER_ID_ERROR"
                    ]
                  },
                  "primaryUserId": {
                    "type": "string",
                    "example": "fa7a0841-b533-4478-9253-0fde890c576"
                  },
                  "description": {
                    "type": "string"
                  }
                }
              },
              {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "string",
                    "enum": [
                      "INPUT_USER_IS_NOT_A_PRIMARY_USER"
                    ]
                  }
                }
              }
            ]
          }
        }
      }
    },
    "400": {
      "description": "error code 400",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string"
          }
        }
      }
    },
    "401": {
      "description": "error code 401",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Invalid API key"
            ]
          }
        }
      }
    },
    "404": {
      "description": "error code 404",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Not Found"
            ]
          }
        }
      }
    },
    "500": {
      "description": "error code 500",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Internal Error"
            ]
          }
        }
      }
    }
  }
}
{
  "path": "/appid-<appId>/requests/stats",
  "method": "get",
  "description": "Get requests stats for last 24 hours\n\n`averageRequestsPerSecond` and `peakRequestsPerSecond` would countain `1440` values corresponding to `now - 1440 minutes` until `now - 1 minute`.\n\nA value of `-1` would mean that there is no data for that minute.\n",
  "headers": {
    "Authorization": {
      "required": false,
      "schema": {
        "type": "string"
      }
    },
    "cdi-version": {
      "description": "X.Y of the X.Y.Z CDI version.",
      "schema": {
        "type": "string"
      }
    }
  },
  "body": {},
  "responses": {
    "200": {
      "description": "The operation was successful",
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "properties": {
              "status": {
                "type": "string",
                "enum": [
                  "OK"
                ]
              },
              "atMinute": {
                "type": "number",
                "example": 16382348
              },
              "averageRequestsPerSecond": {
                "type": "array",
                "items": {
                  "type": "number",
                  "example": 10.5
                }
              },
              "peakRequestsPerSecond": {
                "type": "array",
                "items": {
                  "type": "number",
                  "example": 20
                }
              }
            }
          }
        }
      }
    },
    "400": {
      "description": "error code 400",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string"
          }
        }
      }
    },
    "401": {
      "description": "error code 401",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Invalid API key"
            ]
          }
        }
      }
    },
    "404": {
      "description": "error code 404",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Not Found"
            ]
          }
        }
      }
    },
    "500": {
      "description": "error code 500",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Internal Error"
            ]
          }
        }
      }
    }
  }
}
{
  "path": "/{apiBasePath}/callback/apple",
  "method": "post",
  "description": "Handles sign in with the apple.\n",
  "headers": {},
  "parameters": {
    "apiBasePath": {
      "name": "apiBasePath",
      "in": "path",
      "description": "Its value depends on the apiBasePath set by the user",
      "required": true,
      "schema": {
        "type": "string",
        "example": "auth"
      }
    }
  },
  "body": {
    "schema": {
      "application/x-www-form-urlencoded": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "description": "The Authorization code which will be exchanged for an access token"
          },
          "state": {
            "type": "string",
            "description": "A unique string used to mitigate CSRF attacks"
          }
        }
      }
    }
  },
  "responses": {
    "303": {
      "description": "Redirects the user to the client's app."
    },
    "404": {
      "description": "error code 404",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Not Found"
            ]
          }
        }
      }
    },
    "500": {
      "description": "error code 500",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Internal Error"
            ]
          }
        }
      }
    }
  }
}
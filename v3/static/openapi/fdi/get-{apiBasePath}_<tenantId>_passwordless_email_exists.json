{
  "path": "/{apiBasePath}/<tenantId>/passwordless/email/exists",
  "method": "get",
  "description": "Check if an email exists\n",
  "parameters": {
    "apiBasePath": {
      "name": "apiBasePath",
      "in": "path",
      "description": "Its value depends on the apiBasePath set by the user",
      "required": true,
      "schema": {
        "type": "string",
        "example": "auth"
      }
    },
    "rid": {
      "name": "rid",
      "in": "header",
      "example": "passwordless",
      "schema": {
        "type": "string"
      }
    },
    "email": {
      "name": "email",
      "in": "query",
      "required": true,
      "schema": {
        "type": "string",
        "example": "johndoe@gmail.com"
      }
    }
  },
  "body": {},
  "responses": {
    "200": {
      "description": "Checks if an email exists",
      "content": {
        "application/json": {
          "schema": [
            {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "enum": [
                    "OK"
                  ]
                },
                "exists": {
                  "type": "boolean",
                  "example": true
                }
              }
            },
            {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "enum": [
                    "GENERAL_ERROR"
                  ]
                },
                "message": {
                  "type": "string"
                }
              }
            }
          ]
        }
      }
    },
    "404": {
      "description": "error code 404",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Not Found"
            ]
          }
        }
      }
    },
    "500": {
      "description": "error code 500",
      "content": {
        "text/plain": {
          "schema": {
            "type": "string",
            "enum": [
              "Internal Error"
            ]
          }
        }
      }
    }
  }
}